{
  "manifest_version": 3,
  "name": "ePhishSense",
  "version": "1.1",
  "description": "Detect phishing emails",
  "permissions": [
    "activeTab",
    "storage", 
    "https://ephishsense.onrender.com/",
    "http://127.0.0.1:5000/"],
  "host_permissions": ["https://ephishsense.onrender.com/", "http://127.0.0.1:5000/"] ,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.jpeg"
    }
  },
  "icons": {
    "16": "images/icon16.jpeg"
  }
}

